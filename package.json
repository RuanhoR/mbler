    "build": "node bin/minify.js"
  },
  "dependencies": {
    "@babel/generator": "7.28.5",
    "@babel/parser": "7.28.5",
    "@babel/traverse": "7.28.5",
    "@babel/types": "7.28.6",
    "@mbler/mcx-core": "latest",
    "adm-zip": "0.5.16",
    "chokidar": "4.0.3",
    "dayjs": "1.11.18",
    "terser": "5.44.0",
