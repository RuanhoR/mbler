  },
  "files": [
    "lib",
    "test",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "bin": {
    "mbler": "bin/mbler.js"
  },
  "devDependencies": {
    "@minecraft/server": "2.4.0",
    "@types/adm-zip": "0.5.7",
    "@types/babel__generator": "7.27.0",
    "@types/babel__traverse": "7.28.0",
    "@types/node": "25.0.9",
    "globals": "17.0.0"
  }
