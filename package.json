{
  "name": "mbler",
  "version": "0.1.0",
  "description": "一个针对Minecraft JavaScript API 的lib库，让你的附加包版本管理更简单",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RuanhoR/mbler.git"
  },
  "keywords": [
    "Minecraft",
    "build"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "license": "MIT",
  "type": "commonjs",
  "main": "./lib/start/index.js",
  "scripts": {
    "cli": "node bin/mbler.js",
    "build": "node bin/minify.js"
  },
  "dependencies": {
    "@babel/generator": "7.28.5",
    "@babel/parser": "7.28.5",
    "@babel/traverse": "7.28.5",
    "@minecraft/server": "^2.4.0",
    "adm-zip": "^0.5.16",
    "chokidar": "^4.0.3",
    "dayjs": "1.11.18",
    "mime-types": "3.0.1",
    "terser": "5.44.0",
    "typescript": "^5.9.3"
  },
  "files": [
    "lib",
    "test",
    "bin"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "bin": {
    "mbler": "bin/mbler.js"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^25.0.9"
  }
}
